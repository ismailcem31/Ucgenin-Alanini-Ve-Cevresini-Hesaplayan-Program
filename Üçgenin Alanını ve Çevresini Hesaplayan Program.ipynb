    "yukseklik = float(input(\"Lütfen bu kenara ait yüksekliği girin: \"))\n",
    " \n",
    "# Alanı hesapla\n",
    "alan = (kenar * yukseklik) / 2\n",
    " \n",
    "print(f\"Üçgenin Alanı: {alan}\")"
   ]
